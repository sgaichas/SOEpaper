	author = {Zador, Stephani G. and Holsman, Kirstin K. and Aydin, Kerim Y. and Gaichas, Sarah K.},
	month = aug,
	year = {2016},
	keywords = {Ecosystem-based fisheries management, ecosystem approach to fisheries, Alaska, ecosystem assessment, ecosystem report cards.},
	pages = {fsw144},
